{
    "name": "MainSystemDev/laravel-jwt-auth",
    "description": "JSON Web Token Authentication for Laravel 4 and 5. Modification from Tymon's jwt-auth",
    "keywords": [
        "jwt",
        "auth",
        "authentication",
        "tymon",
        "laravel",
        "json web token",
        "json web encryption",
        "jwe"
    ],
    "homepage": "https://github.com/MainSystemDev/laravel-jwt-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "John Turingan",
            "email": "john.turingan@gmail.com",
            "homepage": "https://github.com/johnturingan",
            "role": "Developer"
        },
        {
            "name": "Rowen Pimentel",
            "email": "rowenpimentel@gmail.com",
            "homepage": "https://github.com/rowen17",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/http": "~5.0",
        "Spomky-Labs/jose": "3.0.*",
        "fgrosse/phpasn1": "dev-compat/php5-5 as v1.3.1",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "illuminate/auth": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/console" : "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tymon\\JWTAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tymon\\JWTAuth\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5-dev"
        }
    }
}
